{
  "name": "suas-monitor",
  "version": "1.0.0",
  "description": "Monitor Redux for native Apps",
  "main": "main.ts",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack && concurrently 'webpack --watch' 'electron .'",
    "lint": "tslint --type-check --project tsconfig.json",
    "start": "NODE_ENV=production && electron .",
    "test": "jest",
    "package-mac": "electron-packager . Suas-Monitor --platform=darwin --arch=x64 --out ./out/build/ --app-version 0.1.0 --overwrite --icon=./assets/icon/icon.icns",
    "package-win-32": "electron-packager . Suas-Monitor --platform=win32 --arch=ia32 --out ./out/build/ --app-version 0.1.0 --overwrite",
    "package-linux-32": "electron-packager . Suas-Monitor --platform=linux --arch=ia32 --out ./out/build/ --app-version 0.1.0 --overwrite --icon=./assets/icon/icon.icns",
    "package-win-64": "electron-packager . Suas-Monitor --platform=win32 --arch=x64 --out ./out/build/ --app-version 0.1.0 --overwrite",
    "package-linux-64": "electron-packager . Suas-Monitor --platform=linux --arch=x64 --out ./out/build/ --app-version 0.1.0 --overwrite --icon=./assets/icon/icon.icns",
    "package": "npm run package-mac && npm run package-win-32 && npm run package-win-64 && npm run package-linux-32 && npm run package-linux-64",
    "package-mac-zip": "cd out/build/Suas-Monitor-darwin-x64 && zip -r Suas-Monitor.zip Suas-Monitor.app"
  },
  "keywords": [
    "redux",
    "swift",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zendesk/Suas-Monitor.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bonjour": "^3.5.4",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.15",
    "@types/react": "^15.0.38",
    "@types/react-test-renderer": "^15.5.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "electron": "~1.6.11",
    "electron-packager": "^8.7.2",
    "file-loader": "^0.11.2",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.1",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.2",
    "webpack": "^3.2.0"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "bonjour": "^3.5.0",
    "electron-prebuilt": "^1.4.13",
    "inline-style-prefixer": "^3.0.6",
    "milligram": "^1.3.0",
    "normalize.css": "^7.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.2",
    "react-notify-toast": "^0.3.1",
    "react-redux": "^5.0.5",
    "react-tooltip": "^3.3.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-chart-monitor": "^1.6.1",
    "redux-devtools-diff-monitor": "^5.0.5",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-inspector": "^0.11.3",
    "redux-logger": "^3.0.6",
    "styled-components": "^2.1.1"
  }
}
